<% content_for :title, "Signup" %>
<div class="container">
   <div class="row">
      <div class="col-md-6 col-md-offset-3">
         <h1 class="text-center">Signup</h1>
         <%= form_for(@user) do |f| %>
         <%= render "shared/error_messages", object: f.object %>
         <%= render "users/form", f: f %>
         <%= f.submit "Create Account", class: "btn btn-primary" %>
         <% end %>
      </div>
   </div>
</div>